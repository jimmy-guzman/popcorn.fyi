{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "cSpell.words": [
    "devicon",
    "dotenvx",
    "eslintcache",
    "gitzy",
    "iconify",
    "knip",
    "lefthook",
    "noninteractive",
    "nvmrc",
    "TMDB",
    "TURSO",
    "valibot",
    "vercel",
    "vinxi",
    "VITE"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "flags": ["unstable_ts_config"]
  }
}
